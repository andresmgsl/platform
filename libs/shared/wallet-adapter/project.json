{
  "projectType": "library",
  "root": "libs/shared/wallet-adapter",
  "sourceRoot": "libs/shared/wallet-adapter/src",
  "prefix": "heavy-duty",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": ["dist/libs/shared/wallet-adapter"],
      "options": {
        "project": "libs/shared/wallet-adapter/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/shared/wallet-adapter/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/shared/wallet-adapter/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shared/wallet-adapter"],
      "options": {
        "jestConfig": "libs/shared/wallet-adapter/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/shared/wallet-adapter/src/**/*.ts",
          "libs/shared/wallet-adapter/src/**/*.html"
        ]
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version"
    }
  },
  "tags": []
}
